{
  "name": "divertise-asia-site",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run start\" \"npm run server\"",
    "staging-dev": "concurrently --kill-others \"npm run start\" \"npm run server:staging\"",
    "server": "webpack-dev-server --open --host 0.0.0.0 --port 8080 --public localhost:8080 --config webpack.dev.js",
    "server:staging": "cross-env API_SERVER=staging webpack-dev-server --open --host 0.0.0.0 --port 8080 --public localhost:8080 --config webpack.dev.js",
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "test": "echo \"No test specified\"",
    "webpack": "webpack -w",
    "webpack:production": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "keywords": [
    "BuckleScript",
    "Reason"
  ],
  "author": "Divertise Asia Co., Ltd.",
  "license": "MIT",
  "dependencies": {
    "@divertiseasia/bs-divertise-core-components": "^1.0.0",
    "bs-css": "^7.2.0",
    "glslify": "^7.0.0",
    "glslify-import-loader": "^0.1.2",
    "glslify-loader": "^2.0.0",
    "jest": "^26.4.0",
    "mocha": "^8.1.1",
    "raw-loader": "^4.0.0",
    "react": "^16.13.1",
    "reason-react": ">=0.8.0",
    "three": "^0.113.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "bs-platform": "^7.3.2",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.0",
    "react-dom": "^16.13.1",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.4"
  }
}
